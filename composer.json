{
    "name": "ultrono/laravel-sitemap",
    "description": "PHP 8 sitemap generator for Laravel 8 and 9",
    "keywords": ["laravelium", "laravel", "php", "sitemap", "generator", "xml", "html", "google-news"],
    "license": "MIT",
    "authors": [
        {
            "name": "Rumen Damyanov",
            "email": "r@alfamatter.com",
            "role": "Developer",
            "homepage": "https://darumen.com"
        },
        {
            "name": "Rob Allport",
            "email": "rob@f9web.co.uk",
            "role": "Contributor",
            "homepage": "https://f9web.co.uk"
        }
    ],
    "support": {
        "issues": "https://github.com/ultrono/laravel-sitemap/issues",
        "source": "https://github.com/ultrono/laravel-sitemap",
        "wiki": "https://github.com/ultrono/laravel-sitemap/wiki"
    },
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0 || ^9.0",
        "illuminate/filesystem": "^8.0 || ^9.0"
    },
    "require-dev": {
        "laravel/framework": "^8.0 || ^9.0",
        "phpunit/phpunit": "^9.3",
        "orchestra/testbench-core": "^6.0 || ^7.0",
        "php-coveralls/php-coveralls" : "^2.2"
    },
    "autoload": {
        "psr-0": {
            "Laravelium\\Sitemap": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laravelium\\Sitemap\\SitemapServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
